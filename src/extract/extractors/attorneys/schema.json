{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "attorneys",
    "total_attorneys"
  ],
  "properties": {
    "attorneys": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "title": {
            "type": [
              "string",
              "null"
            ]
          },
          "bio_snippet": {
            "type": [
              "string",
              "null"
            ]
          }
        },
        "required": [
          "name"
        ]
      }
    },
    "total_attorneys": {
      "type": "integer",
      "minimum": 0
    }
  }
}